        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyD8n4R0uIU9sbk12Bwx8U7UCQGcGZbg1Bc"
        }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Esta aplicación usa la ubicación para mostrar centros de rehabilitación cercanos."
        }
      ]
    ]
  }
}
